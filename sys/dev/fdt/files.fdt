device	dwmmc: sdmmcbus
attach	dwmmc at fdt
file	dev/fdt/dwmmc.c			dwmmc

device	dwdog
attach	dwdog at fdt
file	dev/fdt/dwdog.c			dwdog

device	mvclock
attach	mvclock at fdt
