attach	moscom at uhub
file	dev/usb/moscom.c		moscom

# iPAQ PDAs
# Generic ipaq support
device	uipaq: ucombus
attach	uipaq at uhub
file	dev/usb/uipaq.c			uipaq

# Qualcomm MSM EVDO
device	umsm: ucombus
attach	umsm at uhub
file	dev/usb/umsm.c			umsm

# WinChipHead CH341/340 serial
