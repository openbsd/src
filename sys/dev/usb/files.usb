# USB logical device
device	usbf {}
attach	usbf at usbdev
file	dev/usb/usbf.c			usbf			needs-flag
file	dev/usb/usbf_subr.c		usbf

# Communication Device Class Ethernet function
device	cdcef {}
attach	cdcef at usbf
file	dev/usb/if_cdcef.c		cdcef

