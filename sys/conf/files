# generic attributes
define	disk
define	tape
define	ifnet
define	tty
define	audio {}
define	scsi {}
define	ifmedia
define	mii {[phy = -1]}
define	midibus { }
define	midisyn
define	radiobus {}

# audio device attributes
define	mulaw
define	auconv

# radio device attributes
define tea5757
define lm700x
define tc921x
define pt2254a

# audio and midi devices, attaches to audio hardware driver
device	audio
attach	audio at audio
device	midi
attach	midi at midibus

# raster operations attributes
define	rasops1
define	rasops2
define	rasops4
define	rasops8
define	rasops15
define	rasops16
