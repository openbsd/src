com*		at omap?		# onboard uarts
ommmc*		at omap?		# SD/MMC card controller
sdmmc*		at ommmc?		# SD/MMC bus

ehci*		at omap?		# EHCI (shim)
usb*		at ehci?

# Sunxi A1x/A20 SoC
sunxi0		at mainbus?

# Sunxi on-chip devices
