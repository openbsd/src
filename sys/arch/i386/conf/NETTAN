ie1	at isa? port 0x300 irq 10		# EtherExpress
le0	at isa? port 0x360 irq 15 drq 6		# IsoLan, NE2100, and DEPCA
ep0	at eisa? slot ?
ep*	at eisa? slot ?				# 3C579 ethernet cards
fea*	at eisa? slot ?				# DEC DEFEA FDDI cards
