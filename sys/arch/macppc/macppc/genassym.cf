struct	pmap
member	PM_SR		pm_sr[0]
member	PM_USRSR	pm_sr[USER_SR]
member	PM_KERNELSR	pm_sr[KERNEL_SR]

struct	proc
member	p_forw
member	p_back
member	p_addr
