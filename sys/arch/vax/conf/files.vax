file	arch/vax/uba/tm.c		te needs-count

# TS11 at UBA
device	ts at uba
file	arch/vax/uba/ts.c		ts needs-count

# TU78 at MBA
device	mu at mba
file	arch/vax/mba/mt.c		mu needs-count

# KDB50 on BI
