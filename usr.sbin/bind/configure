if test "yes" = "$enable_sit"
then
	case $with_sit_alg in
		*1)
			with_sit_alg="sha1"
			;;
		*2*)
			with_sit_alg="sha256"
			;;
		auto)
			if test "no" != "$with_aes"
			then
				with_aes="yes"
			fi
			;;
		*)
			with_sit_alg="aes"
