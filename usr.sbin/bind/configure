$as_echo "#define WITH_IDN 1" >>confdefs.h

	STD_CINCLUDES="$STD_CINCLUDES -I$idn_path/include"
	if test "no" != "$idnlib"; then
		IDNLIBS="$idnlib $iconvlib"
	else
		IDNLIBS="-L$idn_path/lib -lidnkit $iconvlib"
	fi
fi


