# Leave nc=16; adjust size using: ns
rdroot|ramdiskroot|RAM-disk root FS image:\
	:dt=rdroot:se#512:nc#16:nt#2:ns#768:\
	:ta=4.2BSD:oa#0:pa#24576:fa#512:ba#4096:\
	:oc#0:pc#24576:

miniroot:\
	:dt=rdroot:se#512:nc#33:nt#16:ns#128:\
	:oc#0:pc#67584:
