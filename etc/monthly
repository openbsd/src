#!/bin/sh -
#	$OpenBSD: monthly,v 1.3 1996/05/26 10:25:32 deraadt Exp $

host=`hostname -s`
echo "Subject: $host monthly run output"

if [ -f /etc/monthly.local ];then
	echo ""
	echo "Running monthly.local:"
	. /etc/monthly.local
else
	echo "Nothing to do!"
fi

