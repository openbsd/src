#
# Makefile.in for top-level of pcc.
#

@SET_MAKE@
CC=@CC@
CFLAGS=@CFLAGS@
LDFLAGS=@LDFLAGS@
CPPFLAGS=@CPPFLAGS@
YACC=@YACC@
LEX=@LEX@

SUBDIR=cc cpp ccom

all: ${SUBDIR}

install:
	cd cc && ${MAKE} install
	cd cpp && ${MAKE} install
	cd ccom && ${MAKE} install

clean:
	cd cc && ${MAKE} clean
	cd cpp && ${MAKE} clean
	cd ccom && ${MAKE} clean

distclean:
	cd cc && ${MAKE} distclean
	cd cpp && ${MAKE} distclean
	cd ccom && ${MAKE} distclean
	/bin/rm -rf Makefile config.log stamp-h1 config.status \
	configure.lineno config.h autom4te.cache

${SUBDIR}:
	cd $@; $(MAKE) all $(MFLAGS)
