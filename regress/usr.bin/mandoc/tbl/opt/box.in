.\" $OpenBSD: box.in,v 1.5 2022/08/28 11:45:27 schwarze Exp $
.TH TBL-OPT-BOX 1 "August 28, 2022"
.SH NAME
tbl-opt-box \- box table options
.SH DESCRIPTION
no boxing:
.TS
tab(:);
l l.
a:b
c:d
.TE
automatic boxing:
.TS
tab(:) box;
l l.
a:b
c:d
.TE
allbox:
.TS
tab(:) allbox;
l l.
a:b
c:d
.TE
manual boxing:
.TS
tab(:);
||l||l||.
_
a:b
_
c:d
_
.TE
automatic and manual boxing:
.TS
tab(:) box;
||l||l||.
a:b
_
c:d
_
.TE
allbox and manual boxing:
.TS
tab(:) allbox;
||l||l||.
a:b
_
c:d
_
.TE
