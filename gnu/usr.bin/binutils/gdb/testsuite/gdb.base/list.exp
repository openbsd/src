	-re ".*$prompt $" { fail "list 18-27" ; return }
	timeout { fail "list 18-27 (timeout)" ; return }
    }

    send "list -\n"
    expect {
	-re "8\[ \t\]+breakpoint\[(\]\[)\];.*17\[ \t\]+foo \[(\]+.*\[)\]+;\r\n$prompt $" { incr testcnt }
	-re ".*$prompt $" { fail "list 8-17" ; return }
	timeout { fail "list 8-17 (timeout)" ; return }
